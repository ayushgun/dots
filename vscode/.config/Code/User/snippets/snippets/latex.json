{
  // Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }

  "Document": {
    "prefix": "doc",
    "body": [
      "\\documentclass[12pt]{article}\n\\usepackage{parskip}\n\\usepackage{mdframed}\n\\usepackage[a4paper,\n            left=1in,\n            right=1in,\n            top=1in,\n            bottom=1in,\n            footskip=.25in]{geometry}\n\n\\begin{document}\n\\begin{mdframed}[linewidth=0.5pt, innerleftmargin=10pt, innerrightmargin=10pt, innertopmargin=10pt, innerbottommargin=10pt]\n    \\hfill \\large Course Number -- Semester\n    \\begin{center}\n        \\huge Document Title\n    \\end{center}\n\n    \\large Ayush Gundawar\n    \\hfill\n    \\large \\today\n\\end{mdframed}\n\n\\section*{Section}\nSome content here.\n\n\\end{document}\n"
    ]
  },
  "Notes": {
    "prefix": "note",
    "body": [
      "\\documentclass[12pt]{article}\n\\usepackage[margin=1in]{geometry}\n\\usepackage{fancyhdr}\n\n\\pagestyle{fancy}\n\\fancyhf{}\n\\lhead{\\today}\n\\chead{\\Large \\scshape Document Title}\n\\rhead{Ayush Gundawar}\n\\cfoot{\\thepage}\n\\setlength{\\headheight}{22pt}\n\\setlength\\parindent{0pt}\n\n\\begin{document}\n\\section*{Section}\nSome content here.\n\n\\end{document}"
    ]
  }
}
